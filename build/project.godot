; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="LifeOfLoader"
run/main_scene="res://scenes/WorkSession.tscn"
boot_splash/image="res://textures/splash/blank_first_screen.png"
boot_splash/bg_color=Color( 0, 0, 0, 0 )

[autoload]

Session="*res://scripts/Session.gd"

[display]

window/size/width=800
window/size/height=400
window/stretch/mode="viewport"

[editor_plugins]

enabled=PoolStringArray(  )

[global]

import=false

[input_devices]

pointing/emulate_touch_from_mouse=true
pointing/emulate_mouse_from_touch=false

[layer_names]

2d_physics/layer_1="walls"
2d_physics/layer_2="shelves"
2d_physics/layer_3="boxes"
2d_physics/layer_4="platform"
2d_physics/layer_5="player"
2d_physics/layer_6="tray"
2d_physics/layer_7="invWall"

[rendering]

2d/snapping/use_gpu_pixel_snap=true

[tiled_importer]

enable_json_format=true
